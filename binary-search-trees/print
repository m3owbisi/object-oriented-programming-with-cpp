#include<iostream>
#include<set>
using namespace std;
int main(){
    set<int> s;
    s.insert(1);
    s.insert(2);
    s.insert(2);
    s.insert(3);
    // print in reverse
    for(auto i=s.rbegin();i!=s.rend();i++)
        cout<<*i<<" ";
    cout<<"\n";
    // size of the set
    cout<<s.size();
    return 0;
}
