#include<iostream>
using namespace std;
int main(){
    char button;
    cout<<"input a character:\t";
    cin>>button;
    if(button=='a'){
        cout<<"hello"<<endl;
    } else if(button=='b'){
        cout<<"namaste"<<endl;
    } else if(button=='c'){
        cout<<"hola"<<endl;
    } else if(button=='d'){
        cout<<"salut"<<endl;
    } else if(button=='e'){
        cout<<"ciao"<<endl;
    } else{
        cout<<"i am still learning more"<<endl;
    }
    return 0;
}
